<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.api.dao.ImgInfoDao">

  <select id="findByFoodId" resultType="com.example.api.entity.ImgInfo">
    select * from dining_img_info
    where food_id = #{foodId}
  </select>

</mapper>